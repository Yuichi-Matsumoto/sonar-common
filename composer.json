{
    "name": "sonar/common",
    "description": "sonar common libraries",
    "keywords": [
        "sonar",
        "common"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuichi Matsumoto",
            "email": "matsumoto@snr.jp",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~6.0",
        "illuminate/filesystem": "~6.0",
        "nesbot/carbon": "2.*",
        "symfony/yaml": "3.*",
        "php" : ">=7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Sonar\\Common\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonar\\Common\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "master": "1.0.0-dev"
        }
    }
}
